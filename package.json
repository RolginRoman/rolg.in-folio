{
  "name": "app",
  "type": "module",
  "packageManager": "yarn@3.5.1",
  "version": "0.0.1",
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "build": "astro build",
    "preview": "astro preview --open",
    "runeslint": "eslint src/**/*.astro",
    "lint": "yarn runeslint && yarn astro check",
    "killdev": "lsof -i :3000 | grep node | awk '{print $2}' | xargs kill -9",
    "dev-watch": "yarn chokidar \"**/*.scss\" -c \"yarn killdev && yarn dev\"",
    "astro": "astro"
  },
  "devDependencies": {
    "@fontsource/inter": "^4.5.15",
    "@typescript-eslint/parser": "^5.59.2",
    "astro": "^2.4.2",
    "astro-emoji": "^1.2.0",
    "astro-icon": "^0.8.0",
    "astro-purgecss": "^2.0.1",
    "chokidar-cli": "^3.0.0",
    "eslint": "^8.40.0",
    "eslint-plugin-astro": "^0.26.1",
    "normalize.css": "^8.0.1",
    "prettier": "^2.8.8",
    "prettier-plugin-astro": "^0.8.0",
    "sass": "^1.62.1"
  }
}
