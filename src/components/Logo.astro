---
import { Icon } from "astro-icon";
export interface Props {
  width?: number | string;
  height?: number | string;
}

let { width, height } = Astro.props;
width ??= 96;
height ??= "auto";
---

<Icon
  name="logo"
  width={width}
  height={width}
  optimize={false}
  aria-label="Rolgin Roman personal logo, `RR` stands for `R`olgin `R`oman"
/>
